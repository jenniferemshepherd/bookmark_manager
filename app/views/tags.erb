<h1> Links with tag 'bubbles' </h1>

<ul id='links'>
  <% @links.each do |link| %>
    <% if link.tags.include?('bubbles')%>
      <li>
        <a href="<%= link.url %>"><%= link.title %></a>
      </li>
    <% end %>
  <% end %>
    </ul>

<form action="/links/new">
  <input type="submit" value="add link">
</form>
